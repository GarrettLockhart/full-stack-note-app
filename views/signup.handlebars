<div class='login-main-container'>
  <div class='login-left-container'>
    <h2 class='login-left-header'>Already with us?</h2>
    <p class='login-left-text'>Log back in and start note taking away!</p>
    <a class='login-btn' href='/login'>Login</a>
  </div>
  <div class='login-right-container'>
    <section class='login-form-container'>
      <form id='register' class='login-main-form-element'>
        <h4>Sign Up for an Account</h4>
        <label for='form-first-name'>First Name</label>
        <input type='text' id='form-first-name' />
        <label for='form-last-name'>Last Name</label>
        <input type='email' id='form-last-name' />
        <label for='form-email'>Email</label>
        <input type='email' id='form-email' />
        <label for='form-password'>Password</label>
        <input type='password' id='form-password' />
        <button class='btn btn-form' type='submit'>Register</button>
        <div id='warning'></div>
      </form>
    </section>
  </div>
</div>

